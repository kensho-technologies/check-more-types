{
  "name": "check-more-types",
  "description": "Large collection of predicates.",
  "version": "0.0.0",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": {
    "url": "https://github.com/kensho/check-more-types/issues"
  },
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm run build",
        "npm test"
      ],
      "pre-push": [
        "npm run size"
      ],
      "post-commit": [
        "git status"
      ],
      "post-merge": []
    },
    "next-update": {
      "skip": [
        "grunt-cli"
      ]
    }
  },
  "contributors": [],
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/condition-travis": "4.1.4",
    "condition-node-version": "1.2.0",
    "coveralls": "2.11.8",
    "git-issues": "1.2.0",
    "grunt-cli": "0.1.13",
    "grunt-contrib-jshint": "1.0.0",
    "grunt-contrib-uglify": "1.0.0",
    "grunt-contrib-watch": "0.6.1",
    "grunt-deps-ok": "0.9.0",
    "grunt-gt": "0.1.6",
    "grunt-jshint-solid": "0.1.1",
    "grunt-mocha-test": "0.12.7",
    "grunt-nice-package": "0.10.2",
    "grunt-npm2bower-sync": "0.9.1",
    "grunt-readme": "0.4.5",
    "grunt-string-replace": "1.2.1",
    "grunt-toc": "0.1.0",
    "grunt-xplain": "0.2.7",
    "jshint-summary": "0.4.0",
    "lazy-ass": "1.4.0",
    "lazy-ass-helpful": "0.6.1",
    "lcov-filter": "0.1.1",
    "lodash": "4.6.1",
    "matchdep": "1.0.1",
    "mocha": "2.4.5",
    "pkgfiles": "2.3.0",
    "pre-git": "3.4.1",
    "semantic-release": "6.2.0",
    "standard": "6.0.8",
    "standard-format": "2.1.1",
    "webpack": "1.12.14"
  },
  "engines": {
    "node": ">= 0.8.0"
  },
  "files": [
    "bower.json",
    "dist/check-more-types.js",
    "dist/check-more-types.min.js"
  ],
  "homepage": "https://github.com/kensho/check-more-types",
  "keywords": [
    "check-types",
    "checks",
    "duck-typing",
    "type-checking",
    "types"
  ],
  "license": "MIT",
  "main": "dist/check-more-types.js",
  "release": {
    "verifyConditions": [
      {
        "path": "@semantic-release/condition-travis"
      },
      {
        "path": "condition-node-version",
        "node": "4.2.2"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kensho/check-more-types.git"
  },
  "scripts": {
    "build": "npm run pack && grunt",
    "commit": "git-issues && commit-wizard",
    "coveralls": "lcov-filter cover/lcov.info test | ./node_modules/coveralls/bin/coveralls.js",
    "format": "standard-format -w src/*.js test/*.js",
    "issues": "git-issues",
    "lint": "standard --verbose src/*.js test/*.js",
    "mocha": "mocha test/check-more-types-spec.js",
    "node-test": "node test/load-under-node-test.js",
    "pack": "webpack",
    "pkgfiles": "pkgfiles",
    "prepublish": "npm run set-version",
    "pretest": "npm run format && npm run lint",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "set-version": "grunt version && grunt",
    "size": "tarball=\"$(npm pack .)\"; wc -c \"${tarball}\"; tar tvf \"${tarball}\"; rm \"${tarball}\";",
    "test": "npm run node-test && npm run mocha",
    "unit": "npm run mocha",
    "watch": "grunt watch"
  }
}
